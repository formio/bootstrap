{
  "name": "@formio/bootstrap",
  "version": "0.0.1",
  "author": "Brendan J Bond <brendanbond@gmail.com>",
  "description": "A basic default template for Form.io forms.",
  "main": "lib/cjs/bootstrap5.js",
  "module": "lib/mjs/bootstrap5.js",
  "exports": {
    ".": {
      "import": "./lib/cjs/bootstrap5.js",
      "require": "./lib/cjs/bootstrap5.js"
    },
    "./bootstrap5": {
      "import": "./lib/cjs/bootstrap5.js",
      "require": "./lib/cjs/bootstrap5.js"
    }
  },
  "typesVersions": {
    "*": {
      "bootstrap5": [
        "lib/mjs/bootstrap5.d.ts"
      ]
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.cjs.json && tsc --project tsconfig.mjs.json",
    "lint": "eslint . --fix",
    "prettier": "prettier --write .",
    "test": "vitest"
  },
  "pre-commit": [
  ],
  "dependencies": {
    "@formio/standard-template": "file:../standard-template"
  },
  "devDependencies": {
    "@formio/core": "^2.3.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "prettier": "^3.4.2",
    "tsc": "^2.0.4",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0",
    "vitest": "^3.0.5"
  }
}

{
  "name": "@formio/bootstrap3",
  "version": "2.2.0",
  "description": "Bootstrap 3 templates for form.io",
  "main": "lib/index.js",
  "scripts": {
    "test:coverage": "nyc --reporter=text mocha --reporter spec './{,!(node_modules)/**/}*.spec.js'",
    "test": "mocha --require ts-node/register --reporter spec './{,!(node_modules)/**/}*.spec.ts'",
    "watch": "tsc -w",
    "build": "tsc && gulp templates && webpack",
    "lint": "tslint -p .",
    "prepublish": "npm run build"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formio/bootstrap3.git"
  },
  "keywords": [
    "Form.io",
    "USWDS",
    "Us",
    "Web",
    "Design",
    "Standards"
  ],
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/ejs": "^3.0.0",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.1",
    "@types/sinon": "^7.5.1",
    "chai": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^2.0.0",
    "gulp-template": "^5.0.0",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "pre-commit": "^1.2.2",
    "sinon": "^8.1.1",
    "ts-node": "^8.6.2",
    "ts-sinon": "^1.0.25",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "author": "Randall Knutson <randall@form.io>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/formio/bootstrap3/issues"
  },
